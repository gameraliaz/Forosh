             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Forosh.MVVM.Views" xmlns:models="clr-namespace:Forosh.MVVM.Models"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="1200"
